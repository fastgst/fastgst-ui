<script>
    // Image Zoom and Pan Functionality
    document.addEventListener('DOMContentLoaded', function() {
        // Only enable on non-mobile devices
        if (window.innerWidth < 640) return;
        
        const zoomWrappers = document.querySelectorAll('.image-zoom-wrapper');
        
        zoomWrappers.forEach(function(wrapper) {
            const container = wrapper.querySelector('.image-zoom-container');
            const overlay = wrapper.querySelector('.image-zoom-overlay');
            const img = container.querySelector('img');
            const imgSrc = img.getAttribute('data-zoom-src');
            
            if (!img || !overlay || !imgSrc) return;
            
            // Set background image for overlay
            overlay.style.backgroundImage = 'url(' + imgSrc + ')';
            
            // Mouse move handler for panning and positioning
            container.addEventListener('mousemove', function(e) {
                const rect = container.getBoundingClientRect();
                const x = e.clientX - rect.left;
                const y = e.clientY - rect.top;
                
                // Calculate percentage position (0-100%)
                const percentX = Math.max(0, Math.min(100, (x / rect.width) * 100));
                const percentY = Math.max(0, Math.min(100, (y / rect.height) * 100));
                
                // Position overlay to the right of the container, centered vertically
                const overlayWidth = window.innerWidth > 1024 ? 400 : 300;
                const overlayHeight = window.innerWidth > 1024 ? 400 : 300;
                const spacing = 20;
                
                // Check available space on right and left
                const spaceOnRight = window.innerWidth - rect.right;
                const spaceOnLeft = rect.left;
                
                // Position overlay where there's more space
                if (spaceOnRight >= overlayWidth + spacing || spaceOnRight > spaceOnLeft) {
                    // Position on the right side
                    overlay.style.left = (rect.right + spacing) + 'px';
                } else {
                    // Position on the left side
                    overlay.style.left = (rect.left - overlayWidth - spacing) + 'px';
                }
                
                // Center vertically relative to container
                let topPosition = rect.top + (rect.height / 2) - (overlayHeight / 2);
                
                // Ensure overlay stays within viewport bounds
                topPosition = Math.max(10, Math.min(topPosition, window.innerHeight - overlayHeight - 10));
                overlay.style.top = topPosition + 'px';
                
                // Pan the zoomed image
                overlay.style.backgroundPosition = percentX + '% ' + percentY + '%';
            });
            
            // Reset on mouse leave
            wrapper.addEventListener('mouseleave', function() {
                overlay.style.backgroundPosition = '50% 50%';
            });
        });
    });
</script>

